        android:pathData="M100 152 q-5,-80 50,-100 "/>
    <path
        android:fillColor="@color/colorTulipStem"
        android:pathData="M100 152 q50,-20 50,-100 "/>
    <path
        android:fillColor="@color/colorTulipLeaf"
        android:pathData="M100 142 q-5,-60 -50,-70 "/>
    <path
        android:fillColor="@color/colorTulipStem"
        android:pathData="M100 142 q-40,-10 -50,-70 "/>
    <!-- Head Leaves -->

    <path
        android:fillColor="@color/colorTulipShadow"
        android:pathData="M100 80 c-60,-20 -20,-60 25,-65 c10,20 -0,60 -15,55"/>
    <path
        android:fillColor="@color/colorTulipShadow"
        android:pathData="M100 80 c-60,-20 -20,-60 5,-80"/>
    <path
