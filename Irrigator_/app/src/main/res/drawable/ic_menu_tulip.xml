    <!-- Pot -->
    <path
        android:fillColor="@color/colorPotBase"
        android:pathData="@string/pot"/>
    <path
        android:fillColor="@color/colorPotShadow"
        android:pathData="@string/potShadow"/>
    <path
        android:fillColor="@color/colorPotTop"
        android:pathData="@string/potTop"/>


</vector>
