<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="200dp"
    android:height="200dp"
    android:viewportWidth="200.0"
    android:viewportHeight="200.0">
    <!-- Leaves -->
    <path
        android:fillColor="@color/colorFlowerStem"
        android:pathData="M100 140 q15,-40 40,-50 "/>
    <path
        android:fillColor="@color/colorFlowerStemShadow"
        android:pathData="M100 140 q40,-20 40,-50 "/>
    <!-- Stem -->
    <path
        android:strokeColor="@color/colorFlowerStemShadow"
        android:strokeWidth="3"
        android:pathData="M100 140 c20,-30 0,-90 0,-90"/>
    <!-- Head Leaves -->
    <path
        android:fillColor="@color/colorFlowerLeafShadow"
        android:pathData="M100 60
        c20,-20 20,-40 0,-60
        c-20,20 -20,40 0,60 "/>
    <path
        android:fillColor="@color/colorFlowerLeaf"
        android:pathData="M100 60
        c20,-20 20,-40 0,-60
        c-12,20 -12,40 0,60 "/>
    <path
        android:fillColor="@color/colorFlowerLeafShadow"
        android:pathData="M100 60
        c-5,-20 -25,-40 -55,-30
        c5,20 25,40 55,30"/>
    <path
        android:fillColor="@color/colorFlowerLeaf"
        android:pathData="M100 60
        c-5,-20 -25,-40 -55,-30
        c5,20 50,40 55,30"/>
    <path
        android:fillColor="@color/colorFlowerLeafShadow"
        android:pathData="M100 60
        c5,-20 25,-40 55,-30
        c-5,20 -25,40 -55,30"/>
    <path
        android:fillColor="@color/colorFlowerLeaf"
        android:pathData="M100 60
        c5,-20 25,-40 55,-30
        c-5,20 -50,40 -55,30"/>
    <path
        android:fillColor="@color/colorFlowerLeafShadow"
        android:pathData="M150 90
        c-5,-20 -25,-40 -55,-30
        c5,20 25,40 55,30"/>
    <path
        android:fillColor="@color/colorFlowerLeaf"
        android:pathData="M150 90
        c-5,-20 -25,-40 -55,-30
        c5,20 40,35 55,30"/>
    <path
        android:fillColor="@color/colorFlowerLeafShadow"
        android:pathData="M50 90
        c5,-20 25,-40 55,-30
        c-5,20 -25,40 -55,30"/>
    <path
        android:fillColor="@color/colorFlowerLeaf"
        android:pathData="M50 90
        c5,-20 25,-40 55,-30
        c-5,20 -50,35 -55,30"/>
    <path
        android:fillColor="@color/colorFlowerLeafShadow"
        android:pathData="M100 60
        c-20,20 -20,40 0,60
        c20,-20 20,-40 0,-60 "/>
    <path
        android:fillColor="@color/colorFlowerLeaf"
        android:pathData="M100 60
        c-20,20 -20,40 0,60
        c12,-20 12,-40 0,-60 "/>
    <!-- Head -->
    <path
        android:fillColor="@color/colorFlowerHead"
        android:pathData="M100,45 a 20,20 0 0,1 0,40 a20,20 0 0,1 0,-40"/>
    <!-- Eyes -->
    <path
        android:fillColor="#1e1204"
        android:pathData="M90,60 a2,2 0 0,1 0,4 a2,2 0 0,1 0,-4"/>
    <path
        android:fillColor="#1e1204"
        android:pathData="M110,60 a2,2 0 0,1 0,4 a2,2 0 0,1 0,-4"/>
    <!-- Mouth -->
    <path
        android:fillColor="@color/colorSmiley"
        android:pathData="M93,70 a7,7 0 0,0 14,0"/>
    <path
        android:fillColor="@color/colorFlowerLeafTongue"
        android:pathData="M97,74.5 a3,2 0 0,1 6,0 a3,1 0 0,1 -6,0"/>
    <!-- Pot -->
    <path
        android:fillColor="@color/colorPotBase"
        android:pathData="@string/pot"/>
    <path
        android:fillColor="@color/colorPotShadow"
        android:pathData="@string/potShadow"/>
    <path
        android:fillColor="@color/colorPotTop"
        android:pathData="@string/potTop"/>
</vector>
